            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="addAdresseResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
                <xs:element name="adresseType" type="tns:adresseType" />
            </xs:sequence>
        </xs:complexType>
