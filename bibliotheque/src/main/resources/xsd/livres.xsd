<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://www.bibliotheque.com/livres-ws"
           targetNamespace="http://www.bibliotheque.com/livres-ws"
           elementFormDefault="qualified">


    <!-- ==== STATUT ==== -->

    <xs:complexType name="serviceStatus">
        <xs:sequence>
            <xs:element name="statusCode" type="xs:string"/>
            <xs:element name="message" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

    <!-- ========================= -->
    <!-- ==== SERVICES LIVRE ===== -->
    <!-- ========================= -->

    <!-- ==== LIVRE TYPE ==== -->

    <xs:complexType name="livreType">
        <xs:sequence>
            <xs:element name="id" type="xs:int" />
            <xs:element name="titre" type="xs:string" />
            <xs:element name="auteur" type="xs:string" />
            <xs:element name="genre" type="xs:string" />
            <xs:element name="date_publication" type="xs:date" />
            <xs:element name="resume" type="xs:string" />
            <xs:element name="bibliothequeid" type="xs:int" />
            <xs:element name="bibliotheque" type="tns:bibliothequeType"/>


        </xs:sequence>
    </xs:complexType>

    <!-- ==== GET LIVRE BY ID ==== -->

    <xs:element name="getLivreByIdRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getLivreByIdResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="livreType" type="tns:livreType" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <!-- ==== GET ALL LIVRES ==== -->

    <xs:element name="getAllLivresRequest">
        <xs:complexType/>
    </xs:element>
    <xs:element name="getAllLivresResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="livreType" type="tns:livreType" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== ADD LIVRE ==== -->

    <xs:element name="addLivreRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int" />
                <xs:element name="titre" type="xs:string" />
                <xs:element name="auteur" type="xs:string" />
                <xs:element name="genre" type="xs:string" />
                <xs:element name="date_publication" type="xs:date" />
                <xs:element name="resume" type="xs:string" />
                <xs:element name="url_photo" type="xs:string" />
                <xs:element name="bibliothequeid" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="addLivreResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
                <xs:element name="livreType" type="tns:livreType"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== UPDATE LIVRE ==== -->

    <xs:element name="updateLivreRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int" />
                <xs:element name="titre" type="xs:string" />
                <xs:element name="auteur" type="xs:string" />
                <xs:element name="genre" type="xs:string" />
                <xs:element name="date_publication" type="xs:date" />
                <xs:element name="resume" type="xs:string" />
                <xs:element name="url_photo" type="xs:string" />
                <xs:element name="bibliothequeid" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="updateLivreResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== DELETE LIVRE ==== -->

    <xs:element name="deleteLivreRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="deleteLivreResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ========================= -->
    <!-- ==== SERVICES MEMBRE===== -->
    <!-- ========================= -->

    <!-- ==== TYPE MEMBRE ==== -->

    <xs:complexType name="membreType">
        <xs:sequence>
            <xs:element name="id" type="xs:int" />
            <xs:element name="nom" type="xs:string" />
            <xs:element name="prenom" type="xs:string" />
            <xs:element name="adresse_mail" type="xs:string" />
            <xs:element name="mot_de_passe" type="xs:string" />
            <xs:element name="telephone" type="xs:string" />
            <xs:element name="num_carte_bibliotheque" type="xs:string" />
            <xs:element name="adresseid" type="xs:int" />
            <xs:element name="adresse" type="tns:adresseType" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==== GET MEMBRE BY ID ==== -->

    <xs:element name="getMembreByIdRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getMembreByIdResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="membreType" type="tns:membreType" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== GET ALL MEMBRES ==== -->

    <xs:element name="getAllMembresRequest">
        <xs:complexType/>
    </xs:element>
    <xs:element name="getAllMembresResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="membreType" type="tns:membreType" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== ADD MEMBRE ==== -->

    <xs:element name="addMembreRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="membreType" type="tns:membreType" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="addMembreResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
                <xs:element name="membreType" type="tns:membreType" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== UPDATE MEMBRE ==== -->

    <xs:element name="updateMembreRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="membreType" type="tns:membreType" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="updateMembreResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== DELETE MEMBRE ==== -->

    <xs:element name="deleteMembreRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="deleteMembreResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <!-- ================================ -->
    <!-- ==== SERVICES BIBLIOTHEQUE ===== -->
    <!-- ================================ -->


    <!-- ==== BIBLIOTHEQUE TYPE ==== -->

    <xs:complexType name="bibliothequeType">
    <xs:sequence>
    <xs:element name="id" type="xs:int" />
    <xs:element name="nom" type="xs:string" />
    <xs:element name="telephone" type="xs:string" />
        <xs:element name="adresseid" type="xs:int" />
        <xs:element name="adresse" type="tns:adresseType" />
        <xs:element name="listeLivres" type="tns:livreType" />


    </xs:sequence>
    </xs:complexType>

    <!-- ==== GET BIBLIOTHEQUE BY ID ==== -->

    <xs:element name="getBibliothequeByIdRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getBibliothequeByIdResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="bibliothequeType" type="tns:bibliothequeType" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== GET ALL BIBLIOTHEQUES ==== -->

    <xs:element name="getAllBibliothequesRequest">
        <xs:complexType/>
    </xs:element>
    <xs:element name="getAllBibliothequesResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="bibliothequeType" type="tns:bibliothequeType" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== ADD BIBLIOTHEQUE ==== -->

    <xs:element name="addBibliothequeRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="bibliothequeType" type="tns:bibliothequeType" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="addBibliothequeResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
                <xs:element name="bibliothequeType" type="tns:bibliothequeType" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== UPDATE BIBLIOTHEQUE ==== -->

    <xs:element name="updateBibliothequeRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="bibliothequeType" type="tns:bibliothequeType" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="updateBibliothequeResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ==== DELETE BIBLIOTHEQUE ==== -->

    <xs:element name="deleteBibliothequeRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int"/>
